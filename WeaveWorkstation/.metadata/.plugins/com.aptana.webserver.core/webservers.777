<?xml version="1.0" encoding="UTF-8"?>
<servers>
<server type="com.aptana.webserver.external">
<name>Aptana Tomcat</name>
<baseURL>http://localhost:8080</baseURL>
<documentRoot>file:/Library/Tomcat/webapps/ROOT</documentRoot>
<startCommand>/Library/Tomcat/bin/startup.sh</startCommand>
<stopCommand>/Library/Tomcat/bin/shutdown.sh</stopCommand>
</server>
</servers>