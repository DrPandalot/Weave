{
  "name": "weavejs",
  "version": "0.0.0",
  "description": "A session and data framework for visualization on the web",
  "main": "WeaveJS/bin/js-release/WeaveJS.js",
  "dependencies": {
    "as2dts": "^0.2.3",
    "flexjs": "^0.5.5"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "mxmlc -remove-circulars -js-compiler-options=\"--compilation_level WHITESPACE_ONLY --output_wrapper_file umdWrapper.js\" -fb ./WeaveJS/",
    "compile-dts": "as2dts --defs-only ./WeaveJS/ ./WeaveJS-Typing/src && tsc -d -p ./WeaveJS-Typing/",
    "prepublish": "npm run compile && npm run compile-dts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WeaveTeam/Weave.git"
  },
  "keywords": [
    "visualization",
    "weave",
    "history",
    "data"
  ],
  "author": "npm@iweave.com",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/WeaveTeam/Weave/issues"
  },
  "homepage": "https://www.iweave.com"
}
