<mx:Button 	xmlns:mx="http://www.adobe.com/2006/mxml" 
			label="Weave \u00A9 {(new Date()).getFullYear()} IVPR, UMass Lowell"
			skin="{null}"
			fontFamily="Verdana, Tahoma, Arial"
			fontWeight="normal"
			fontSize="10"
	       	fontStyle="normal"
			textDecoration="none" 
			color="#000000" 
			useHandCursor="true"			
	       	paddingLeft="1" paddingRight="1" paddingTop="0" 
	       	
	       	click="navigateToURL(new URLRequest('http://www.oicweave.org/'), '_blank');"
	       	fillAlphas="[0.8, 1.0, 1.0, 1.0]" 
	       	repeatDelay="0" 
	       	cornerRadius="0" 
	       	height="{this.getStyle('font-size') + 2}" 
	       	textAlign="center"
	       	styleName="application"
>
			       	<!--creationCompleteEffect="{logoGlow}"-->

   <mx:Script>
   	<![CDATA[
   	]]>
   </mx:Script>
</mx:Button>
