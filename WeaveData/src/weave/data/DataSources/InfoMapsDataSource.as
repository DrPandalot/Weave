		private var parser:CSVParser = new CSVParser();
		
		private function removeEmptyStringElementsFromArray(arg:Array):Array
		{
			var result:Array = [];
			for each(var item:String in arg)
			{
				if(item != "")
					result.push(item);
			}
			
			return result;
		}
		
		
